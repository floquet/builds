  if test "${abi_fc_vendor}" != "unknown"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: applying Wannier90 tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&5
$as_echo "$as_me: applying Wannier90 tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&6;}
  fi

  CPPFLAGS_WANNIER90="${lib_linalg_incs} ${CPPFLAGS_WANNIER90}"
  FCLIBS_WANNIER90="${lib_linalg_libs} ${FCLIBS_WANNIER90}"

  case "${abi_fc_vendor}" in

    intel)
      case "${target_cpu}" in
        ia64)
          # Do nothing
          ;;
        *)
          FCLIBS_WANNIER90="${FCLIBS_WANNIER90} -lsvml"
          ;;
      esac
      ;;

  esac

    if test "${wannier90_tricks_bypass}" = "yes"; then
      build_wannier90="no"
    fi
  fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable the WANNIER90 plug-in" >&5
$as_echo_n "checking whether to enable the WANNIER90 plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${enable_wannier90}" >&5
$as_echo "${enable_wannier90}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to build the WANNIER90 plug-in" >&5
$as_echo_n "checking whether to build the WANNIER90 plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${build_wannier90}" >&5
$as_echo "${build_wannier90}" >&6; }

    if test "${enable_wannier90}" = "yes"; then

$as_echo "#define HAVE_WANNIER90 1" >>confdefs.h

  fi





     if test "${enable_wannier90}" = "yes"; then
  DO_TEST_WANNIER90_TRUE=
  DO_TEST_WANNIER90_FALSE='#'
else
