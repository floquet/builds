$as_echo "$as_me: fcflags_opt_28_numeric_noabirule overriden by FCFLAGS" >&6;}
    fi
  fi


    if test "${FCFLAGS}" = ""; then
    if test "${fcflags_opt_32_contract}" = "" -o "${enable_optim}" = "no"; then
      fcflags_opt_32_contract="${fcflags_opt_default}"
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: optimization for 32_contract is ${fcflags_opt_32_contract}" >&5
$as_echo "$as_me: optimization for 32_contract is ${fcflags_opt_32_contract}" >&6;}
      echo "fcflags_opt_32_contract='${fcflags_opt_32_contract}'" >>config.optim
      fcflags_opt_dirlist="${fcflags_opt_dirlist} 32_contract"
    fi
