  abi_trio_netcdf_mpi="yes"
else
  abi_trio_netcdf_mpi="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_trio_netcdf_has_libs}" >&5
$as_echo "${abi_trio_netcdf_has_libs}" >&6; }
  fi

    LIBS="${abi_trio_netcdf_libs} ${LIBS}"

          if test "${abi_trio_netcdf_serial}" = "yes"; then

$as_echo "#define HAVE_TRIO_NETCDF 1" >>confdefs.h

            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: disabling NetCDF plugin" >&5
$as_echo "$as_me: WARNING: disabling NetCDF plugin" >&2;}
            enable_netcdf="no"

$as_echo "#define HAVE_NETCDF 1" >>confdefs.h

          else
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: falling back to NetCDF plugin" >&5
$as_echo "$as_me: WARNING: falling back to NetCDF plugin" >&2;}
            enable_netcdf="yes"
          fi
          ;;

        *)
          as_fn_error "unknown TRIO flavor '${abi_trio_flavor}'" "$LINENO" 5
          ;;

      esac

    done

        if test "${abi_trio_hdf_serial}" = "no"; then
      abi_trio_iter=`echo "${abi_trio_iter}" | sed -e 's/hdf//'`
    fi
    lib_trio_flavor=`echo "${abi_trio_iter}" | sed -e 's/^[ ]*//;s/[ ]*$//;s/ [ ]*/+/g;'`
