  abi_dft_wannier90_libs="${with_dft_libs}"

    if test "${abi_dft_wannier90_libs}" = ""; then
    LIBS="-lwannier ${LIBS}"
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether we have a working Wannier90 library" >&5
$as_echo_n "checking whether we have a working Wannier90 library... " >&6; }
