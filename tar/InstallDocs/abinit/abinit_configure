if test "${abi_fc_vendor}" = "sun" -a \( "${enable_debug}" = "no" -o \
        "${enable_debug}" = "yes" \); then
  enable_etsf_io="no"
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: ETSF I/O cannot be built by Sun compilers => disabled" >&5
$as_echo "$as_me: WARNING: ETSF I/O cannot be built by Sun compilers => disabled" >&2;}
fi

    lib_etsf_io_incs=""
  lib_etsf_io_libs=""
  build_etsf_io="no"
  abi_plug_download="no"
  abi_plug_ready="no"
  abi_plug_tarball="no"
  abi_plug_bins=""
  abi_plug_incs=""
  abi_plug_libs=""

    if test "${WGET}" != ""; then
    abi_plug_downloader="${WGET}"
    abi_plug_dlopts="--timeout=15 --tries=1 -q -O"
  elif test "${CURL}" != ""; then
    abi_plug_downloader="${CURL}"
    abi_plug_dlopts="--connect-time 15 --retry 1 -s -S -o"
  fi

    etsf_io_pkg_name="etsf_io-1.0.3"

  etsf_io_pkg_string="ETSF I/O library 1.0.3 (upstream release)"


  if test -z "${CFGFLAGS_ETSF_IO}"; then
    CFGFLAGS_ETSF_IO=""
  fi

  if test -z "${CPPFLAGS_ETSF_IO}"; then
    CPPFLAGS_ETSF_IO=""
  fi

  if test -z "${CFLAGS_ETSF_IO}"; then
    CFLAGS_ETSF_IO="${CFLAGS}"
  fi

  if test -z "${CXXFLAGS_ETSF_IO}"; then
    CXXFLAGS_ETSF_IO="${CXXFLAGS}"
  fi

  if test -z "${FCFLAGS_ETSF_IO}"; then
    FCFLAGS_ETSF_IO="${FCFLAGS}"
  fi


    FCFLAGS_ETSF_IO="${FCFLAGS_ETSF_IO} ${fcflags_opt_etsf_io}"

    if test "${enable_etsf_io}" = "yes"; then

        if test "${with_plugins_prefix}" != ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether ETSF_IO is ready for use" >&5
$as_echo_n "checking whether ETSF_IO is ready for use... " >&6; }
      abi_plug_ready="yes"
      abi_plug_bins="${with_plugins_prefix}/bin"
      abi_plug_incs="-I${with_plugins_prefix}/include"
      abi_plug_libs="-L${with_plugins_prefix}/lib"

      if test ! -x "${with_plugins_prefix}/bin/etsf_io"; then
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibetsf_io.a.a"; then
        abi_plug_libs="${abi_plug_libs} -llibetsf_io.a"
      else
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibetsf_io_utils.a.a"; then
        abi_plug_libs="${abi_plug_libs} -llibetsf_io_utils.a"
