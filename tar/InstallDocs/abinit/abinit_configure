                  ;;
                safe)
                  enable_optim_opt="safe"
                  CXXFLAGS_OPTIM="-O2 -mcpu=pentiumpro -msse -xK"
                  ;;
                standard)
                  enable_optim_opt="standard"
                  CXXFLAGS_OPTIM="-O2 -mcpu=pentiumpro -msse -xK"
                  ;;
