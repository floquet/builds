    conftest$ac_exeext conftest.$ac_ext
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_trio_fox_has_libs}" >&5
$as_echo "${abi_trio_fox_has_libs}" >&6; }
  fi

    if test "${abi_trio_fox_has_incs}" = "yes" -a \
          "${abi_trio_fox_has_libs}" = "yes"; then
    abi_trio_fox_serial="yes"
  fi

          if test "${abi_trio_fox_serial}" = "yes"; then

$as_echo "#define HAVE_TRIO_FOX 1" >>confdefs.h

                        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: disabling FoX plugin" >&5
