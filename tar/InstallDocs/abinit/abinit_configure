# External FFT support

    lib_fft_flavor="${with_fft_flavor}"
  lib_fft_fcflags=""
  lib_fft_ldflags=""
  lib_fft_incs=""
  lib_fft_libs=""
  abi_fft_serial="no"
  abi_fft_mpi="no"

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to use optimized FFT libraries" >&5
$as_echo_n "checking whether to use optimized FFT libraries... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${enable_fft}" >&5
$as_echo "${enable_fft}" >&6; }



    abi_env_ABI_CPPFLAGS="${ABI_CPPFLAGS}"

    abi_env_AR="${AR}"

