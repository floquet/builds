  case $abi_fpp_path in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_abi_fpp_path="$abi_fpp_path" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
