    "src/14_hidewrite/Makefile") CONFIG_FILES="$CONFIG_FILES src/14_hidewrite/Makefile" ;;
    "src/15_gpu_toolbox/Makefile") CONFIG_FILES="$CONFIG_FILES src/15_gpu_toolbox/Makefile" ;;
