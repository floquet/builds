      fi
    done
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid include statement in with_netcdf_includes: ${v}" "$LINENO" 5
    fi
  fi

   if test "${with_netcdf_libs}" != ""; then
    opt_ok="yes"
    for v in ${with_netcdf_libs}; do
      if test "`echo "${v}" | grep '^-[LlW]'`" = ""; then
        opt_ok="no"
        break
      fi
    done
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid library statement in with_netcdf_libs: ${v}" "$LINENO" 5
    fi
  fi

   if test "${with_test_timeout}" != ""; then
    opt_ok="yes"
    test "`echo "${with_test_timeout}" | ${EGREP} -e '^-?[0-9]+$'`" = "" && opt_ok="no"
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid integer: with_test_timeout = ${with_test_timeout}" "$LINENO" 5
    fi
  fi

    if test "${with_timer_flavor}" != ""; then
    for v in `echo "${with_timer_flavor}" | sed -e 's/+/ /g'`; do
      opt_ok="no"
      for r in abinit gptl papi; do
        if test "${v}" = "${r}";then
          opt_ok="yes"
          break
        fi
      done
      test "${opt_ok}" = "no" && break
    done
    if test "${opt_ok}" = "no"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: with_timer_flavor = (abinit|gptl|papi)" >&5
$as_echo "$as_me: WARNING: with_timer_flavor = (abinit|gptl|papi)" >&2;}
      as_fn_error "invalid option: with_timer_flavor = ${v}" "$LINENO" 5
    fi
  fi

   if test "${with_timer_includes}" != ""; then
    opt_ok="yes"
    for v in ${with_timer_includes}; do
      if test "`echo "${v}" | grep '^-I'`" = ""; then
        opt_ok="no"
        break
      fi
    done
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid include statement in with_timer_includes: ${v}" "$LINENO" 5
    fi
  fi

   if test "${with_timer_libs}" != ""; then
    opt_ok="yes"
    for v in ${with_timer_libs}; do
      if test "`echo "${v}" | grep '^-[LlW]'`" = ""; then
        opt_ok="no"
        break
      fi
    done
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid library statement in with_timer_libs: ${v}" "$LINENO" 5
    fi
  fi

    if test "${with_trio_flavor}" != ""; then
    for v in `echo "${with_trio_flavor}" | sed -e 's/+/ /g'`; do
      opt_ok="no"
      for r in etsf fox hdf netcdf; do
        if test "${v}" = "${r}";then
          opt_ok="yes"
          break
        fi
      done
      test "${opt_ok}" = "no" && break
    done
    if test "${opt_ok}" = "no"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: with_trio_flavor = (etsf|fox|hdf|netcdf)" >&5
$as_echo "$as_me: WARNING: with_trio_flavor = (etsf|fox|hdf|netcdf)" >&2;}
      as_fn_error "invalid option: with_trio_flavor = ${v}" "$LINENO" 5
    fi
  fi


   if test "${with_trio_libs}" != ""; then
    opt_ok="yes"
    for v in ${with_trio_libs}; do
      if test "`echo "${v}" | grep '^-[LlW]'`" = ""; then
        opt_ok="no"
        break
      fi
    done
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid library statement in with_trio_libs: ${v}" "$LINENO" 5
    fi
  fi

   if test "${with_wannier90_includes}" != ""; then
    opt_ok="yes"
    for v in ${with_wannier90_includes}; do
      if test "`echo "${v}" | grep '^-I'`" = ""; then
        opt_ok="no"
        break
      fi
    done
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid include statement in with_wannier90_includes: ${v}" "$LINENO" 5
    fi
  fi

   if test "${with_wannier90_libs}" != ""; then
    opt_ok="yes"
    for v in ${with_wannier90_libs}; do
      if test "`echo "${v}" | grep '^-[LlW]'`" = ""; then
        opt_ok="no"
        break
      fi
    done
    if test "${opt_ok}" = "no"; then
      as_fn_error "invalid library statement in with_wannier90_libs: ${v}" "$LINENO" 5
    fi
  fi



# ---------------------------------------------------------------------------- #

#
# Connector startup
#



  abi_sec_title="Connector startup"

    abi_sec_spaces="                                                                      "
  abi_sec_tmp="${abi_sec_title}"
  while test "${abi_sec_tmp}" != ""; do
    abi_sec_spaces=`echo "${abi_sec_spaces}" | sed -e 's/^.//'`
    abi_sec_tmp=`echo "${abi_sec_tmp}" | sed -e 's/^.//'`
  done

  echo ""
  echo " =============================================================================="
  echo " === ${abi_sec_title}${abi_sec_spaces} ==="
  echo " =============================================================================="
  echo ""


# Prepare MPI support

    abi_mpi_complete="unknown"
  abi_mpi_has_cc="no"
  abi_mpi_has_cxx="no"
  abi_mpi_has_fc="no"
  abi_mpi_has_runner="no"
  abi_mpi_level="${with_mpi_level}"
  abi_mpi_usable="no"
  lib_mpi_incs="${with_mpi_includes}"
  lib_mpi_libs="${with_mpi_libs}"
  test "${MPI_RUNNER}" = "" && MPI_RUNNER="${with_mpi_runner}"

  if test "${enable_mpi}" = "yes"; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: Initializing MPI support" >&5
$as_echo "$as_me: Initializing MPI support" >&6;}

        if test "${with_mpi_prefix}" != ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: looking for MPI in ${with_mpi_prefix}" >&5
$as_echo "$as_me: looking for MPI in ${with_mpi_prefix}" >&6;}

            if test "${CPP}" != ""; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: ${CPP} might not be fully compatible with MPI" >&5
$as_echo "$as_me: WARNING: ${CPP} might not be fully compatible with MPI" >&2;}
      fi
      if test "${CC}" != ""; then
        as_fn_error "use --with-mpi-prefix or set CC, not both" "$LINENO" 5
      fi
      if test "${CXX}" != ""; then
        as_fn_error "use --with-mpi-prefix or set CXX, not both" "$LINENO" 5
      fi
      if test "${FC}" != ""; then
        as_fn_error "use --with-mpi-prefix or set FC, not both" "$LINENO" 5
      fi
      if test "${MPI_RUNNER}" != ""; then
        as_fn_error "use --with-mpi-prefix or set MPI_RUNNER, not both" "$LINENO" 5
      fi
      if test "${with_mpi_includes}" != ""; then
        as_fn_error "use --with-mpi-prefix or --with-mpi-includes, not both" "$LINENO" 5
      fi
      if test "${with_mpi_level}" != ""; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: forcing MPI level to ${with_mpi_level} might make the build fail" >&5
$as_echo "$as_me: WARNING: forcing MPI level to ${with_mpi_level} might make the build fail" >&2;}
      fi
      if test "${with_mpi_libs}" != ""; then
        as_fn_error "use --with-mpi-prefix or --with-mpi-libs, not both" "$LINENO" 5
      fi
      if test "${with_mpi_runner}" != ""; then
        as_fn_error "use --with-mpi-prefix or --with-mpi-runner, not both" "$LINENO" 5
      fi

            { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a MPI C compiler" >&5
$as_echo_n "checking for a MPI C compiler... " >&6; }
      if test -x "${with_mpi_prefix}/bin/mpicc"; then
        abi_mpi_has_cc="yes"
        CC="${with_mpi_prefix}/bin/mpicc"
      fi
      if test "${CC}" = ""; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: none found" >&5
$as_echo "none found" >&6; }
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${CC}" >&5
$as_echo "${CC}" >&6; }
      fi

            { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a MPI C++ compiler" >&5
$as_echo_n "checking for a MPI C++ compiler... " >&6; }
      if test -x "${with_mpi_prefix}/bin/mpicxx"; then
        abi_mpi_has_cxx="yes"
        CXX="${with_mpi_prefix}/bin/mpicxx"
      elif test -x "${with_mpi_prefix}/bin/mpic++"; then
        abi_mpi_has_cxx="yes"
        CXX="${with_mpi_prefix}/bin/mpic++"
      fi
      if test "${CXX}" = ""; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: none found" >&5
$as_echo "none found" >&6; }
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${CXX}" >&5
$as_echo "${CXX}" >&6; }
      fi

            { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a MPI Fortran compiler" >&5
$as_echo_n "checking for a MPI Fortran compiler... " >&6; }
      if test -x "${with_mpi_prefix}/bin/mpif90"; then
        abi_mpi_has_fc="yes"
        FC="${with_mpi_prefix}/bin/mpif90"
      fi
      if test "${FC}" = ""; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: none found" >&5
$as_echo "none found" >&6; }
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${FC}" >&5
$as_echo "${FC}" >&6; }
      fi

            { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a MPI runner" >&5
$as_echo_n "checking for a MPI runner... " >&6; }
      if test -x "${with_mpi_prefix}/bin/mpirun"; then
        abi_mpi_has_runner="yes"
        MPI_RUNNER="${with_mpi_prefix}/bin/mpirun"
      elif test -x "${with_mpi_prefix}/bin/mpiexec"; then
        abi_mpi_has_runner="yes"
        MPI_RUNNER="${with_mpi_prefix}/bin/mpiexec"
      elif test -x "${with_mpi_prefix}/bin/dmpirun"; then
        abi_mpi_has_runner="yes"
        MPI_RUNNER="${with_mpi_prefix}/bin/dmpirun"
      fi
      if test "${MPI_RUNNER}" = ""; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: none found" >&5
$as_echo "none found" >&6; }
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${MPI_RUNNER}" >&5
$as_echo "${MPI_RUNNER}" >&6; }
      fi

            if test "${abi_mpi_has_cc}" = "yes" -a \
                      "${abi_mpi_has_fc}" = "yes" -a \
                      "${abi_mpi_has_runner}" = "yes"; then
        abi_mpi_complete="yes"
      else
        unset CC
        unset CXX
        unset FC
        unset MPI_RUNNER
        abi_mpi_complete="no"
      fi

    else

            if test "${MPI_RUNNER}" = ""; then
        for ac_prog in mpirun mpiexec dmpirun
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_prog_MPI_RUNNER+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$MPI_RUNNER"; then
  ac_cv_prog_MPI_RUNNER="$MPI_RUNNER" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_prog_MPI_RUNNER="$ac_prog"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
MPI_RUNNER=$ac_cv_prog_MPI_RUNNER
if test -n "$MPI_RUNNER"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MPI_RUNNER" >&5
$as_echo "$MPI_RUNNER" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$MPI_RUNNER" && break
done

        if test "${MPI_RUNNER}" != ""; then
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: MPI runner ${MPI_RUNNER} may be incompatible with MPI compilers" >&5
$as_echo "$as_me: WARNING: MPI runner ${MPI_RUNNER} may be incompatible with MPI compilers" >&2;}
        fi
      else
        abi_mpi_runner_works="no"
        if test -x "${MPI_RUNNER}"; then
          abi_mpi_runner_works="yes"
        else
          unset abi_mpi_runner_works
          # Extract the first word of "${MPI_RUNNER}", so it can be a program name with args.
set dummy ${MPI_RUNNER}; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_prog_abi_mpi_runner_works+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$abi_mpi_runner_works"; then
  ac_cv_prog_abi_mpi_runner_works="$abi_mpi_runner_works" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_prog_abi_mpi_runner_works="yes"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_prog_abi_mpi_runner_works" && ac_cv_prog_abi_mpi_runner_works="no"
fi
fi
abi_mpi_runner_works=$ac_cv_prog_abi_mpi_runner_works
if test -n "$abi_mpi_runner_works"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $abi_mpi_runner_works" >&5
$as_echo "$abi_mpi_runner_works" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


        fi
        if test "${abi_mpi_runner_works}" = "no"; then
          as_fn_error "invalid MPI runner: ${MPI_RUNNER}" "$LINENO" 5
        fi
      fi

            { $as_echo "$as_me:${as_lineno-$LINENO}: compiler checks deferred" >&5
$as_echo "$as_me: compiler checks deferred" >&6;}

            if test "${MPI_RUNNER}" != ""; then
        abi_mpi_complete="yes"
      else
        abi_mpi_complete="no"
      fi

    fi
        if test "${enable_mpi_io}" = "" -a "${enable_mpi_io_test}" = "yes"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: setting MPI I/O main trigger" >&5
$as_echo "$as_me: setting MPI I/O main trigger" >&6;}
      enable_mpi_io="yes"
    fi

  else

    { $as_echo "$as_me:${as_lineno-$LINENO}: MPI support disabled from command-line" >&5
$as_echo "$as_me: MPI support disabled from command-line" >&6;}
    enable_mpi_io="no"
    enable_mpi_io_test="no"
    enable_mpi_trace="no"
    with_mpi_level=""
    with_mpi_prefix=""

  fi





# Prepare GPU support

    abi_gpu_complete="unknown"
  abi_gpu_has_cc="no"
  abi_gpu_has_fft="no"
  abi_gpu_has_incs="no"
  abi_gpu_has_libs="no"
  abi_gpu_has_linalg="no"
  abi_gpu_usable="no"
  lib_gpu_fcflags=""
  lib_gpu_ldflags=""
  lib_gpu_flavor="none"
  lib_gpu_incs=""
  lib_gpu_libs=""

  if test "${enable_gpu}" = "yes"; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: Initializing GPU support" >&5
$as_echo "$as_me: Initializing GPU support" >&6;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking which kind of GPU we want" >&5
$as_echo_n "checking which kind of GPU we want... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${with_gpu_flavor}" >&5
$as_echo "${with_gpu_flavor}" >&6; }

        if test "${with_gpu_prefix}" != ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: looking for GPU support in ${with_gpu_prefix}" >&5
$as_echo "$as_me: looking for GPU support in ${with_gpu_prefix}" >&6;}

            if test "${with_gpu_includes}" != ""; then
        as_fn_error "use --with-gpu-prefix or --with-gpu-includes, not both" "$LINENO" 5
      fi
      if test "${with_gpu_libs}" != ""; then
        as_fn_error "use --with-gpu-prefix or --with-gpu-libs, not both" "$LINENO" 5
      fi
    fi

        case "${with_gpu_flavor}" in

      cuda*)

    abi_gpu_cuda_has_cc="no"
  abi_gpu_cuda_has_fft="no"
  abi_gpu_cuda_has_incs="no"
  abi_gpu_cuda_has_libs="no"
  abi_gpu_cuda_has_linalg="no"
  abi_gpu_cuda_libdir=""
  abi_gpu_cuda_incs=""
  abi_gpu_cuda_libs=""

    if test "${with_gpu_prefix}" != ""; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the nVidia C compiler" >&5
$as_echo_n "checking for the nVidia C compiler... " >&6; }
    if test -x "${with_gpu_prefix}/bin/nvcc"; then
      abi_gpu_cuda_has_cc="yes"
      NVCC="${with_gpu_prefix}/bin/nvcc"
    fi
    if test "${NVCC}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none found" >&5
$as_echo "none found" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${NVCC}" >&5
$as_echo "${NVCC}" >&6; }
    fi

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for Cuda includes" >&5
$as_echo_n "checking for Cuda includes... " >&6; }
    abi_result="none"
    if test -s "${with_gpu_prefix}/include/cuda_runtime_api.h"; then
      abi_gpu_cuda_incs="-I${with_gpu_prefix}/include"
      abi_gpu_cuda_has_incs="yes"
      abi_result="run-time"
    fi
    if test -s "${with_gpu_prefix}/include/cufft.h"; then
      abi_result="${abi_result} fft"
    fi
    if test -s "${with_gpu_prefix}/include/cublas.h"; then
      abi_result="${abi_result} blas"
    fi
    if test -s "${with_gpu_prefix}/SDK/C/common/inc/cutil.h"; then
      abi_gpu_cuda_incs="-I${with_gpu_prefix}/SDK/C/common/inc ${abi_gpu_cuda_incs}"
      abi_result="${abi_result} sdk"
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_result}" >&5
$as_echo "${abi_result}" >&6; }

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for Cuda libraries" >&5
$as_echo_n "checking for Cuda libraries... " >&6; }
    abi_result="none"
    if test "${abi_cpu_64bits}" = "yes"; then
      abi_gpu_cuda_libdir="${with_gpu_prefix}/lib64"
    else
      abi_gpu_cuda_libdir="${with_gpu_prefix}/lib"
    fi
    if test -s "${abi_gpu_cuda_libdir}/libcudart.so"; then
      abi_gpu_cuda_libs="-lcudart"
      abi_gpu_cuda_has_libs="yes"
      abi_result="run-time"
    fi
    if test "${abi_gpu_cuda_has_libs}" = "yes"; then
      if test -s "${abi_gpu_cuda_libdir}/libcufft.so"; then
        abi_gpu_cuda_libs="-lcufft ${abi_gpu_cuda_libs}"
        abi_gpu_cuda_has_fft="yes"
        abi_result="${abi_result} fft"
      fi
      if test -s "${abi_gpu_cuda_libdir}/libcublas.so"; then
        abi_gpu_cuda_libs="-lcublas ${abi_gpu_cuda_libs}"
        abi_gpu_cuda_has_linalg="yes"
        abi_result="${abi_result} blas"
      fi
      abi_gpu_cuda_libs="-L${abi_gpu_cuda_libdir} ${abi_gpu_cuda_libs}"
    fi
    if test -s "${with_gpu_prefix}/SDK/C/lib/libcutil.a"; then
      abi_gpu_cuda_libs="-L${with_gpu_prefix}/SDK/C/lib -lcutil ${abi_gpu_cuda_libs}"
      abi_result="${abi_result} sdk"
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_result}" >&5
$as_echo "${abi_result}" >&6; }

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for Cuda link flags" >&5
$as_echo_n "checking for Cuda link flags... " >&6; }
    if test "${CC_LDFLAGS_GPU}" = ""; then
      if test "${abi_cpu_64bits}" = "yes"; then
        CC_LDFLAGS_GPU="-Wl,-rpath=${with_gpu_prefix}/lib64"
      else
        CC_LDFLAGS_GPU="-Wl,-rpath=${with_gpu_prefix}/lib"
      fi
    fi
    if test "${CXX_LDFLAGS_GPU}" = ""; then
      if test "${abi_cpu_64bits}" = "yes"; then
        CXX_LDFLAGS_GPU="-Wl,-rpath=${with_gpu_prefix}/lib64"
      else
        CXX_LDFLAGS_GPU="-Wl,-rpath=${with_gpu_prefix}/lib"
      fi
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${CC_LDFLAGS_GPU}" >&5
$as_echo "${CC_LDFLAGS_GPU}" >&6; }


  else

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the nVidia C compiler" >&5
$as_echo_n "checking for the nVidia C compiler... " >&6; }
    if test "${NVCC}" = ""; then
      for ac_prog in nvcc
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_prog_NVCC+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$NVCC"; then
  ac_cv_prog_NVCC="$NVCC" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_prog_NVCC="$ac_prog"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
NVCC=$ac_cv_prog_NVCC
if test -n "$NVCC"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $NVCC" >&5
$as_echo "$NVCC" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$NVCC" && break
done

      if test "${NVCC}" != ""; then
        abi_gpu_cuda_has_cc="yes"
      fi
    fi

  fi
        abi_gpu_has_cc="${abi_gpu_cuda_has_cc}"
        abi_gpu_has_fft="${abi_gpu_cuda_has_fft}"
        abi_gpu_has_incs="${abi_gpu_cuda_has_incs}"
        abi_gpu_has_libs="${abi_gpu_cuda_has_libs}"
        abi_gpu_has_linalg="${abi_gpu_cuda_has_linalg}"
        if test "${abi_gpu_has_cc}" = "yes" -a \
                "${abi_gpu_has_incs}" = "yes" -a \
                "${abi_gpu_has_libs}" = "yes"; then
          abi_gpu_complete="yes"
        else
          abi_gpu_complete="no"
        fi
        ;;

    esac

  else

    { $as_echo "$as_me:${as_lineno-$LINENO}: GPU support disabled from command-line" >&5
$as_echo "$as_me: GPU support disabled from command-line" >&6;}

  fi







# ---------------------------------------------------------------------------- #

#
# Miscellaneous utilities
#



  abi_sec_title="Utilities"

    abi_sec_spaces="                                                                      "
  abi_sec_tmp="${abi_sec_title}"
  while test "${abi_sec_tmp}" != ""; do
    abi_sec_spaces=`echo "${abi_sec_spaces}" | sed -e 's/^.//'`
    abi_sec_tmp=`echo "${abi_sec_tmp}" | sed -e 's/^.//'`
  done

  echo ""
  echo " =============================================================================="
  echo " === ${abi_sec_title}${abi_sec_spaces} ==="
  echo " =============================================================================="
  echo ""


# Check for various programs
# Extract the first word of "sh", so it can be a program name with args.
set dummy sh; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_path_BOURNE_SHELL+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  case $BOURNE_SHELL in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_BOURNE_SHELL="$BOURNE_SHELL" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_BOURNE_SHELL="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_path_BOURNE_SHELL" && ac_cv_path_BOURNE_SHELL="/bin/sh"
  ;;
esac
fi
BOURNE_SHELL=$ac_cv_path_BOURNE_SHELL
if test -n "$BOURNE_SHELL"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $BOURNE_SHELL" >&5
$as_echo "$BOURNE_SHELL" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


