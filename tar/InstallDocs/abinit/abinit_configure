  abi_saved_LIBS="${LIBS}"
  CPPFLAGS="${with_fft_includes} ${CPPFLAGS}"
  LDFLAGS="${FC_LDFLAGS}"
  LIBS="${with_fft_libs} ${LIBS}"
  ac_ext=${ac_fc_srcext-f}
ac_compile='$FC -c $FCFLAGS $ac_fcflags_srcext conftest.$ac_ext >&5'
ac_link='$FC -o conftest$ac_exeext $FCFLAGS $LDFLAGS $ac_fcflags_srcext conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_fc_compiler_gnu


    if test "${enable_fft}" = "yes"; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the requested FFT support" >&5
$as_echo_n "checking for the requested FFT support... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${with_fft_flavor}" >&5
$as_echo "${with_fft_flavor}" >&6; }

    case "${with_fft_flavor}" in

      asl)

    abi_fft_asl_serial="no"
  abi_fft_asl_mpi="no"
  abi_fft_asl_fcflags=""
  abi_fft_asl_ldflags=""
  abi_fft_asl_incs=""
  abi_fft_asl_libs=""

    if test "${with_fft_libs}" = ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for zfc3fb in -lasl" >&5
$as_echo_n "checking for zfc3fb in -lasl... " >&6; }
if test "${ac_cv_lib_asl_zfc3fb+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lasl  $LIBS"
cat > conftest.$ac_ext <<_ACEOF
      program main
      call zfc3fb
      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  ac_cv_lib_asl_zfc3fb=yes
else
  ac_cv_lib_asl_zfc3fb=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_asl_zfc3fb" >&5
$as_echo "$ac_cv_lib_asl_zfc3fb" >&6; }
if test "x$ac_cv_lib_asl_zfc3fb" = x""yes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_LIBASL 1
_ACEOF

  LIBS="-lasl $LIBS"

fi

    if test "${ac_cv_lib_asl_zfc3fb}" = "yes"; then
      abi_fft_asl_serial="yes"
      abi_fft_asl_libs="-lasl"
    fi
    if test "${enable_mpi}" = "yes" -a \
            "${abi_fft_asl_serial}" = "yes"; then
      abi_fft_asl_mpi="no"
    fi
  else

    abi_fft_user_serial="no"
  abi_fft_user_mpi="no"
  abi_fft_user_threads="no"

      { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a serial optimized FFT support" >&5
$as_echo_n "checking for a serial optimized FFT support... " >&6; }
  cat > conftest.$ac_ext <<_ACEOF
