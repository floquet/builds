    abi_cc_version="unknown"
    if test "${cc_garbage}" -gt 50; then

$as_echo "#define CC_IBM 1" >>confdefs.h

      abi_cc_vendor="ibm"
      abi_cc_version="unknown"
      abi_result="yes"
    fi
