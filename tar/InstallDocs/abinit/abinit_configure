if test -z "${DO_TEST_ETSF_IO_TRUE}" && test -z "${DO_TEST_ETSF_IO_FALSE}"; then
  as_fn_error "conditional \"DO_TEST_ETSF_IO\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
