                  ;;
                safe)
                  enable_optim_opt="safe"
                  CFLAGS_OPTIM="-O2 -mcpu=pentiumpro -msse -xK"
                  ;;
                standard)
                  enable_optim_opt="standard"
                  CFLAGS_OPTIM="-O2 -mcpu=pentiumpro -msse -xK"
                  ;;
              esac   # [case: enable_optim, indent: 6, item: True]
              ;;
            intel_itanium2)
              abi_cpu_spec_opt="intel_itanium2"
              case "${enable_optim}" in
                aggressive)
