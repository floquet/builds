        abi_plug_ready="no"
      fi

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_plug_ready}" >&5
$as_echo "${abi_plug_ready}" >&6; }
    fi

    if test "${abi_plug_ready}" = "yes"; then
      with_libxc_bins="${abi_plug_bins}"
      with_libxc_includes="${abi_plug_incs}"
      with_libxc_libs="${abi_plug_libs}"
    fi

        if test "${with_libxc_includes}" = "" -o "${with_libxc_libs}" = ""; then

                  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a source tarball of LIBXC" >&5
$as_echo_n "checking for a source tarball of LIBXC... " >&6; }
