              yes)
                abi_plug_tarball="yes"
                { $as_echo "$as_me:${as_lineno-$LINENO}: tarball MD5 check succeeded" >&5
$as_echo "$as_me: tarball MD5 check succeeded" >&6;}
                ;;
              unknown)
                abi_plug_tarball="yes"
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not check integrity of tarball" >&5
$as_echo "$as_me: WARNING: could not check integrity of tarball" >&2;}
                ;;
              no)
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: tarball MD5 check failed" >&5
$as_echo "$as_me: WARNING: tarball MD5 check failed" >&2;}
                ;;
            esac
          fi
        fi

                if test "${abi_plug_tarball}" = "yes"; then
          lib_netcdf_incs="-I\$(abinit_builddir)/plugins/netcdf"
          lib_netcdf_libs="-L\$(abinit_builddir)/plugins/netcdf -lnetcdf"
          build_netcdf="yes"
        else
          lib_netcdf_incs=""
          lib_netcdf_libs=""
          enable_netcdf="no"
          build_netcdf="no"
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not download NETCDF plug-in tarball" >&5
$as_echo "$as_me: WARNING: could not download NETCDF plug-in tarball" >&2;}
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: support for NETCDF plug-in has been disabled" >&5
$as_echo "$as_me: WARNING: support for NETCDF plug-in has been disabled" >&2;}
        fi
      fi
    else
      lib_netcdf_incs="${with_netcdf_includes}"
      lib_netcdf_libs="${with_netcdf_libs}"
      build_netcdf="no"
    fi

        if test "${lib_netcdf_incs}" != ""; then
      if test "${abi_fc_vendor}" = "sun"; then
        tmp_netcdf_incs=`echo "${lib_netcdf_incs}" | sed -e 's/-I/-M/g'`
        lib_netcdf_incs="${lib_netcdf_incs} ${tmp_netcdf_incs}"
      fi
    fi
  else
    enable_netcdf="no"
    build_netcdf="no"
  fi

    FCLIBS_NETCDF=''


    if test "${build_netcdf}" = "yes"; then


  if test "${abi_fc_vendor}" != "unknown"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: applying NetCDF tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&5
$as_echo "$as_me: applying NetCDF tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&6;}
  fi

  CFGFLAGS_NETCDF="${CFGFLAGS_NETCDF} --disable-cxx"
  CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -DNDEBUG"

  case "${abi_fc_vendor}" in

    g95)
      CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -Df2cFortran"
      ;;

    gnu)
      CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -DpgiFortran"
      ;;

    ibm)
      CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -DIBMR2Fortran"
      FCFLAGS_NETCDF="${FCFLAGS_NETCDF} -WF,-DIBMR2Fortran,-DNDEBUG"
      ;;

    intel)
      CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -DpgiFortran"
      case "${abi_fc_version}" in
        9.0|9.1)
          FCFLAGS_NETCDF="${FCFLAGS_NETCDF} -mp"
          ;;
        *)
          FCFLAGS_NETCDF="${FCFLAGS_NETCDF} -ip -no-prec-div"
          ;;
      esac
      ;;

    pathscale)
      CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -Df2cFortran"
      ;;

    pgi)
      CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -DpgiFortran"
      ;;

    sun)
      CPPFLAGS_NETCDF="${CPPFLAGS_NETCDF} -DsunFortran"
      ;;

  esac

    if test "${netcdf_tricks_bypass}" = "yes"; then
      build_netcdf="no"
    fi
  fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable the NETCDF plug-in" >&5
$as_echo_n "checking whether to enable the NETCDF plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${enable_netcdf}" >&5
$as_echo "${enable_netcdf}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to build the NETCDF plug-in" >&5
$as_echo_n "checking whether to build the NETCDF plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${build_netcdf}" >&5
$as_echo "${build_netcdf}" >&6; }

    if test "${enable_netcdf}" = "yes"; then

$as_echo "#define HAVE_NETCDF 1" >>confdefs.h

  fi





     if test "${enable_netcdf}" = "yes"; then
  DO_TEST_NETCDF_TRUE=
