#fi

    lib_fox_incs=""
  lib_fox_libs=""
  build_fox="no"
  abi_plug_download="no"
  abi_plug_ready="no"
  abi_plug_tarball="no"
  abi_plug_bins=""
  abi_plug_incs=""
  abi_plug_libs=""

    if test "${WGET}" != ""; then
    abi_plug_downloader="${WGET}"
    abi_plug_dlopts="--timeout=15 --tries=1 -q -O"
  elif test "${CURL}" != ""; then
    abi_plug_downloader="${CURL}"
    abi_plug_dlopts="--connect-time 15 --retry 1 -s -S -o"
  fi

    fox_pkg_name="FoX-4.0.4"

  fox_pkg_string="FoX Fortran XML library 4.0.4 (upstream release)"


  if test -z "${CFGFLAGS_FOX}"; then
    CFGFLAGS_FOX=""
  fi

  if test -z "${CPPFLAGS_FOX}"; then
    CPPFLAGS_FOX=""
  fi

  if test -z "${CFLAGS_FOX}"; then
    CFLAGS_FOX="${CFLAGS}"
  fi

  if test -z "${CXXFLAGS_FOX}"; then
    CXXFLAGS_FOX="${CXXFLAGS}"
  fi

  if test -z "${FCFLAGS_FOX}"; then
    FCFLAGS_FOX="${FCFLAGS}"
  fi


    FCFLAGS_FOX="${FCFLAGS_FOX} ${fcflags_opt_fox}"

    if test "${enable_fox}" = "yes"; then

        if test "${with_plugins_prefix}" != ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether FOX is ready for use" >&5
$as_echo_n "checking whether FOX is ready for use... " >&6; }
      abi_plug_ready="yes"
      abi_plug_bins="${with_plugins_prefix}/bin"
      abi_plug_incs="-I${with_plugins_prefix}/include"
      abi_plug_libs="-L${with_plugins_prefix}/lib"

      if test ! -x "${with_plugins_prefix}/bin/FoX-config"; then
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibFoX_common.a.a"; then
        abi_plug_libs="${abi_plug_libs} -llibFoX_common.a"
      else
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibFoX_fsys.a.a"; then
        abi_plug_libs="${abi_plug_libs} -llibFoX_fsys.a"
      else
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibFoX_utils.a.a"; then
        abi_plug_libs="${abi_plug_libs} -llibFoX_utils.a"
      else
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibFoX_sax.a.a"; then
        abi_plug_libs="${abi_plug_libs} -llibFoX_sax.a"
      else
        abi_plug_ready="no"
      fi

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_plug_ready}" >&5
$as_echo "${abi_plug_ready}" >&6; }
    fi

    if test "${abi_plug_ready}" = "yes"; then
      with_fox_bins="${abi_plug_bins}"
      with_fox_includes="${abi_plug_incs}"
      with_fox_libs="${abi_plug_libs}"
    fi

        if test "${with_fox_includes}" = "" -o "${with_fox_libs}" = ""; then

                  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a source tarball of FOX" >&5
$as_echo_n "checking for a source tarball of FOX... " >&6; }
      if test -s "${abinit_tardir}/FoX-4.0.4.tar.gz"; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }

    if test "${MD5SUM}" = ""; then
    for ac_prog in md5sum
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_prog_MD5SUM+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$MD5SUM"; then
  ac_cv_prog_MD5SUM="$MD5SUM" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_prog_MD5SUM="$ac_prog"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
MD5SUM=$ac_cv_prog_MD5SUM
if test -n "$MD5SUM"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MD5SUM" >&5
$as_echo "$MD5SUM" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$MD5SUM" && break
done

  fi
  abi_md5_ok="unknown"

    if test "${MD5SUM}" = ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: no MD5 sum checker available" >&5
$as_echo "$as_me: WARNING: no MD5 sum checker available" >&2;}
    tmp_md5_file=""
  else
    tmp_md5_file=`${MD5SUM} ${abinit_tardir}/FoX-4.0.4.tar.gz | awk '{print $1}'`
  fi
  tmp_md5_ref="b7f5ae84f8811f1d60212be25cdbed40"

  if test "${tmp_md5_file}" = "${tmp_md5_ref}"; then
    abi_md5_ok="yes"
  else
    if test "${tmp_md5_file}" != ""; then
      abi_md5_ok="no"
    fi
  fi

        case "${abi_md5_ok}" in
          yes)
            abi_plug_tarball="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: tarball MD5 check succeeded" >&5
$as_echo "$as_me: tarball MD5 check succeeded" >&6;}
            ;;
          unknown)
            abi_plug_tarball="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not check integrity of tarball" >&5
$as_echo "$as_me: WARNING: could not check integrity of tarball" >&2;}
            ;;
          no)
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: tarball MD5 check failed" >&5
$as_echo "$as_me: WARNING: tarball MD5 check failed" >&2;}
            ;;
        esac
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      fi

            if test "${abi_plug_ready}" = "no"; then
        if test "${abi_plug_tarball}" = "no"; then
          { $as_echo "$as_me:${as_lineno-$LINENO}: downloading FOX - this may take a while" >&5
$as_echo "$as_me: downloading FOX - this may take a while" >&6;}

          if test ! -s "${abinit_tardir}/FoX-4.0.4.tar.gz"; then
            { $as_echo "$as_me:${as_lineno-$LINENO}: checking availability of FOX from URL 1" >&5
$as_echo_n "checking availability of FOX from URL 1... " >&6; }
            ${abi_plug_downloader} ${abi_plug_dlopts} \
              "${abinit_tardir}/FoX-4.0.4.tar.gz" \
              'http://source.uszla.me.uk/FoX/FoX-4.0.4.tar.gz'
            test -s "${abinit_tardir}/FoX-4.0.4.tar.gz" && abi_plug_download="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_plug_download}" >&5
$as_echo "${abi_plug_download}" >&6; }
          fi
          if test ! -s "${abinit_tardir}/FoX-4.0.4.tar.gz"; then
            { $as_echo "$as_me:${as_lineno-$LINENO}: checking availability of FOX from URL 2" >&5
$as_echo_n "checking availability of FOX from URL 2... " >&6; }
            ${abi_plug_downloader} ${abi_plug_dlopts} \
              "${abinit_tardir}/FoX-4.0.4.tar.gz" \
              'http://archives.abinit.org/plugins/FoX-4.0.4.tar.gz'
            test -s "${abinit_tardir}/FoX-4.0.4.tar.gz" && abi_plug_download="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_plug_download}" >&5
$as_echo "${abi_plug_download}" >&6; }
          fi


                    if test -s "${abinit_tardir}/FoX-4.0.4.tar.gz"; then

    if test "${MD5SUM}" = ""; then
    for ac_prog in md5sum
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_prog_MD5SUM+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$MD5SUM"; then
  ac_cv_prog_MD5SUM="$MD5SUM" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_prog_MD5SUM="$ac_prog"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
MD5SUM=$ac_cv_prog_MD5SUM
if test -n "$MD5SUM"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MD5SUM" >&5
$as_echo "$MD5SUM" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$MD5SUM" && break
done

  fi
  abi_md5_ok="unknown"

    if test "${MD5SUM}" = ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: no MD5 sum checker available" >&5
$as_echo "$as_me: WARNING: no MD5 sum checker available" >&2;}
    tmp_md5_file=""
  else
    tmp_md5_file=`${MD5SUM} ${abinit_tardir}/FoX-4.0.4.tar.gz | awk '{print $1}'`
  fi
  tmp_md5_ref="b7f5ae84f8811f1d60212be25cdbed40"

  if test "${tmp_md5_file}" = "${tmp_md5_ref}"; then
    abi_md5_ok="yes"
  else
    if test "${tmp_md5_file}" != ""; then
      abi_md5_ok="no"
    fi
  fi

            case "${abi_md5_ok}" in
              yes)
                abi_plug_tarball="yes"
                { $as_echo "$as_me:${as_lineno-$LINENO}: tarball MD5 check succeeded" >&5
$as_echo "$as_me: tarball MD5 check succeeded" >&6;}
                ;;
              unknown)
                abi_plug_tarball="yes"
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not check integrity of tarball" >&5
$as_echo "$as_me: WARNING: could not check integrity of tarball" >&2;}
                ;;
              no)
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: tarball MD5 check failed" >&5
$as_echo "$as_me: WARNING: tarball MD5 check failed" >&2;}
                ;;
            esac
          fi
        fi

                if test "${abi_plug_tarball}" = "yes"; then
          lib_fox_incs="-I\$(abinit_builddir)/plugins/fox"
          lib_fox_libs="-L\$(abinit_builddir)/plugins/fox -lFoX_sax -lFoX_utils -lFoX_fsys -lFoX_common"
          build_fox="yes"
        else
          lib_fox_incs=""
          lib_fox_libs=""
          enable_fox="no"
          build_fox="no"
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not download FOX plug-in tarball" >&5
$as_echo "$as_me: WARNING: could not download FOX plug-in tarball" >&2;}
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: support for FOX plug-in has been disabled" >&5
$as_echo "$as_me: WARNING: support for FOX plug-in has been disabled" >&2;}
        fi
      fi
    else
      lib_fox_incs="${with_fox_includes}"
      lib_fox_libs="${with_fox_libs}"
      build_fox="no"
    fi

        if test "${lib_fox_incs}" != ""; then
      if test "${abi_fc_vendor}" = "sun"; then
        tmp_fox_incs=`echo "${lib_fox_incs}" | sed -e 's/-I/-M/g'`
        lib_fox_incs="${lib_fox_incs} ${tmp_fox_incs}"
      fi
    fi
  else
    enable_fox="no"
    build_fox="no"
  fi

    FCLIBS_FOX=''


    if test "${build_fox}" = "yes"; then


  if test "${abi_fc_vendor}" != "unknown"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: applying FOX tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&5
$as_echo "$as_me: applying FOX tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&6;}
  fi

  CFGFLAGS_FOX="${CFGFLAGS_FOX} --enable-sax"

    if test "${fox_tricks_bypass}" = "yes"; then
      build_fox="no"
    fi
  fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable the FOX plug-in" >&5
$as_echo_n "checking whether to enable the FOX plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${enable_fox}" >&5
$as_echo "${enable_fox}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to build the FOX plug-in" >&5
$as_echo_n "checking whether to build the FOX plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${build_fox}" >&5
$as_echo "${build_fox}" >&6; }

    if test "${enable_fox}" = "yes"; then

$as_echo "#define HAVE_FOX 1" >>confdefs.h

  fi





     if test "${enable_fox}" = "yes"; then
  DO_TEST_FOX_TRUE=
  DO_TEST_FOX_FALSE='#'
else
  DO_TEST_FOX_TRUE='#'
  DO_TEST_FOX_FALSE=
fi

   if test "${build_fox}" = "yes"; then
  DO_BUILD_FOX_TRUE=
  DO_BUILD_FOX_FALSE='#'
else
  DO_BUILD_FOX_TRUE='#'
  DO_BUILD_FOX_FALSE=
fi



# ---------------------------------------------------------------------------- #

#
# NetCDF support
#


    lib_netcdf_incs=""
  lib_netcdf_libs=""
  build_netcdf="no"
  abi_plug_download="no"
  abi_plug_ready="no"
  abi_plug_tarball="no"
  abi_plug_bins=""
  abi_plug_incs=""
  abi_plug_libs=""

    if test "${WGET}" != ""; then
    abi_plug_downloader="${WGET}"
    abi_plug_dlopts="--timeout=15 --tries=1 -q -O"
  elif test "${CURL}" != ""; then
    abi_plug_downloader="${CURL}"
    abi_plug_dlopts="--connect-time 15 --retry 1 -s -S -o"
  fi

    netcdf_pkg_name="netcdf-3.6.3"

  netcdf_pkg_string="NetCDF library 3.6.3 (upstream release)"


  if test -z "${CFGFLAGS_NETCDF}"; then
    CFGFLAGS_NETCDF=""
  fi

  if test -z "${CPPFLAGS_NETCDF}"; then
    CPPFLAGS_NETCDF=""
  fi

  if test -z "${CFLAGS_NETCDF}"; then
    CFLAGS_NETCDF="${CFLAGS}"
  fi

  if test -z "${CXXFLAGS_NETCDF}"; then
    CXXFLAGS_NETCDF="${CXXFLAGS}"
  fi

  if test -z "${FCFLAGS_NETCDF}"; then
    FCFLAGS_NETCDF="${FCFLAGS}"
  fi


    FCFLAGS_NETCDF="${FCFLAGS_NETCDF} ${fcflags_opt_netcdf}"

    if test "${enable_netcdf}" = "yes"; then

        if test "${with_plugins_prefix}" != ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether NETCDF is ready for use" >&5
$as_echo_n "checking whether NETCDF is ready for use... " >&6; }
      abi_plug_ready="yes"
      abi_plug_bins="${with_plugins_prefix}/bin"
      abi_plug_incs="-I${with_plugins_prefix}/include"
      abi_plug_libs="-L${with_plugins_prefix}/lib"

      if test ! -x "${with_plugins_prefix}/bin/ncdump"; then
        abi_plug_ready="no"
      fi

      if test ! -x "${with_plugins_prefix}/bin/ncgen"; then
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibnetcdf.a.a"; then
        abi_plug_libs="${abi_plug_libs} -llibnetcdf.a"
      else
        abi_plug_ready="no"
      fi

      if test -s "${with_plugins_prefix}/lib/liblibnetcdf.la.a"; then
        abi_plug_libs="${abi_plug_libs} -llibnetcdf.la"
      else
        abi_plug_ready="no"
      fi

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_plug_ready}" >&5
$as_echo "${abi_plug_ready}" >&6; }
    fi

    if test "${abi_plug_ready}" = "yes"; then
      with_netcdf_bins="${abi_plug_bins}"
      with_netcdf_includes="${abi_plug_incs}"
      with_netcdf_libs="${abi_plug_libs}"
    fi

        if test "${with_netcdf_includes}" = "" -o "${with_netcdf_libs}" = ""; then

                  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a source tarball of NETCDF" >&5
$as_echo_n "checking for a source tarball of NETCDF... " >&6; }
      if test -s "${abinit_tardir}/netcdf-3.6.3.tar.gz"; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }

    if test "${MD5SUM}" = ""; then
    for ac_prog in md5sum
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_prog_MD5SUM+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$MD5SUM"; then
  ac_cv_prog_MD5SUM="$MD5SUM" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_prog_MD5SUM="$ac_prog"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
MD5SUM=$ac_cv_prog_MD5SUM
if test -n "$MD5SUM"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MD5SUM" >&5
$as_echo "$MD5SUM" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$MD5SUM" && break
done

  fi
  abi_md5_ok="unknown"

    if test "${MD5SUM}" = ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: no MD5 sum checker available" >&5
$as_echo "$as_me: WARNING: no MD5 sum checker available" >&2;}
    tmp_md5_file=""
  else
    tmp_md5_file=`${MD5SUM} ${abinit_tardir}/netcdf-3.6.3.tar.gz | awk '{print $1}'`
  fi
  tmp_md5_ref="334e9bdc010b6cd03fd6531a45fe50ad"

  if test "${tmp_md5_file}" = "${tmp_md5_ref}"; then
    abi_md5_ok="yes"
  else
    if test "${tmp_md5_file}" != ""; then
      abi_md5_ok="no"
    fi
  fi

        case "${abi_md5_ok}" in
          yes)
            abi_plug_tarball="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: tarball MD5 check succeeded" >&5
$as_echo "$as_me: tarball MD5 check succeeded" >&6;}
            ;;
          unknown)
            abi_plug_tarball="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not check integrity of tarball" >&5
$as_echo "$as_me: WARNING: could not check integrity of tarball" >&2;}
            ;;
          no)
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: tarball MD5 check failed" >&5
$as_echo "$as_me: WARNING: tarball MD5 check failed" >&2;}
            ;;
        esac
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      fi

            if test "${abi_plug_ready}" = "no"; then
        if test "${abi_plug_tarball}" = "no"; then
          { $as_echo "$as_me:${as_lineno-$LINENO}: downloading NETCDF - this may take a while" >&5
$as_echo "$as_me: downloading NETCDF - this may take a while" >&6;}

          if test ! -s "${abinit_tardir}/netcdf-3.6.3.tar.gz"; then
            { $as_echo "$as_me:${as_lineno-$LINENO}: checking availability of NETCDF from URL 1" >&5
$as_echo_n "checking availability of NETCDF from URL 1... " >&6; }
            ${abi_plug_downloader} ${abi_plug_dlopts} \
              "${abinit_tardir}/netcdf-3.6.3.tar.gz" \
              'http://www.unidata.ucar.edu/downloads/netcdf/ftp/netcdf-3.6.3.tar.gz'
            test -s "${abinit_tardir}/netcdf-3.6.3.tar.gz" && abi_plug_download="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_plug_download}" >&5
$as_echo "${abi_plug_download}" >&6; }
          fi
          if test ! -s "${abinit_tardir}/netcdf-3.6.3.tar.gz"; then
            { $as_echo "$as_me:${as_lineno-$LINENO}: checking availability of NETCDF from URL 2" >&5
$as_echo_n "checking availability of NETCDF from URL 2... " >&6; }
            ${abi_plug_downloader} ${abi_plug_dlopts} \
              "${abinit_tardir}/netcdf-3.6.3.tar.gz" \
              'http://archives.abinit.org/plugins/netcdf-3.6.3.tar.gz'
            test -s "${abinit_tardir}/netcdf-3.6.3.tar.gz" && abi_plug_download="yes"
            { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_plug_download}" >&5
$as_echo "${abi_plug_download}" >&6; }
          fi


                    if test -s "${abinit_tardir}/netcdf-3.6.3.tar.gz"; then

    if test "${MD5SUM}" = ""; then
    for ac_prog in md5sum
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
