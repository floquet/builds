  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_prog_MD5SUM="$ac_prog"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
MD5SUM=$ac_cv_prog_MD5SUM
if test -n "$MD5SUM"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MD5SUM" >&5
$as_echo "$MD5SUM" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$MD5SUM" && break
done

  fi
  abi_md5_ok="unknown"

    if test "${MD5SUM}" = ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: no MD5 sum checker available" >&5
$as_echo "$as_me: WARNING: no MD5 sum checker available" >&2;}
    tmp_md5_file=""
  else
    tmp_md5_file=`${MD5SUM} ${abinit_tardir}/etsf_io-1.0.3.tar.gz | awk '{print $1}'`
  fi
  tmp_md5_ref="b813aaf5d451e67d42dbb58c167f8d5a"

  if test "${tmp_md5_file}" = "${tmp_md5_ref}"; then
    abi_md5_ok="yes"
  else
    if test "${tmp_md5_file}" != ""; then
      abi_md5_ok="no"
    fi
  fi

            case "${abi_md5_ok}" in
              yes)
                abi_plug_tarball="yes"
                { $as_echo "$as_me:${as_lineno-$LINENO}: tarball MD5 check succeeded" >&5
$as_echo "$as_me: tarball MD5 check succeeded" >&6;}
                ;;
              unknown)
                abi_plug_tarball="yes"
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not check integrity of tarball" >&5
$as_echo "$as_me: WARNING: could not check integrity of tarball" >&2;}
                ;;
              no)
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: tarball MD5 check failed" >&5
$as_echo "$as_me: WARNING: tarball MD5 check failed" >&2;}
                ;;
            esac
          fi
        fi

                if test "${abi_plug_tarball}" = "yes"; then
          lib_etsf_io_incs="-I\$(abinit_builddir)/plugins/etsf_io"
          lib_etsf_io_libs="-L\$(abinit_builddir)/plugins/etsf_io -letsf_io_low_level -letsf_io_utils -letsf_io"
          build_etsf_io="yes"
        else
          lib_etsf_io_incs=""
          lib_etsf_io_libs=""
          enable_etsf_io="no"
          build_etsf_io="no"
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not download ETSF_IO plug-in tarball" >&5
$as_echo "$as_me: WARNING: could not download ETSF_IO plug-in tarball" >&2;}
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: support for ETSF_IO plug-in has been disabled" >&5
$as_echo "$as_me: WARNING: support for ETSF_IO plug-in has been disabled" >&2;}
        fi
      fi
    else
      lib_etsf_io_incs="${with_etsf_io_includes}"
      lib_etsf_io_libs="${with_etsf_io_libs}"
      build_etsf_io="no"
    fi

        if test "${lib_etsf_io_incs}" != ""; then
      if test "${abi_fc_vendor}" = "sun"; then
        tmp_etsf_io_incs=`echo "${lib_etsf_io_incs}" | sed -e 's/-I/-M/g'`
        lib_etsf_io_incs="${lib_etsf_io_incs} ${tmp_etsf_io_incs}"
      fi
    fi
  else
    enable_etsf_io="no"
    build_etsf_io="no"
  fi

    FCLIBS_ETSF_IO=''


    if test "${build_etsf_io}" = "yes"; then


  if test "${abi_fc_vendor}" != "unknown"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: applying ETSF_IO tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&5
$as_echo "$as_me: applying ETSF_IO tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&6;}
  fi

  case "${abi_fc_vendor}" in

    ibm)
      FCFLAGS_ETSF_IO="${FCFLAGS_ETSF_IO} -qsuffix=cpp=f90:f=f"
      ;;

  esac

    if test "${etsf_io_tricks_bypass}" = "yes"; then
      build_etsf_io="no"
    fi
  fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable the ETSF_IO plug-in" >&5
$as_echo_n "checking whether to enable the ETSF_IO plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${enable_etsf_io}" >&5
$as_echo "${enable_etsf_io}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to build the ETSF_IO plug-in" >&5
$as_echo_n "checking whether to build the ETSF_IO plug-in... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${build_etsf_io}" >&5
$as_echo "${build_etsf_io}" >&6; }

    if test "${enable_etsf_io}" = "yes"; then

$as_echo "#define HAVE_ETSF_IO 1" >>confdefs.h

  fi





     if test "${enable_etsf_io}" = "yes"; then
  DO_TEST_ETSF_IO_TRUE=
  DO_TEST_ETSF_IO_FALSE='#'
else
  DO_TEST_ETSF_IO_TRUE='#'
  DO_TEST_ETSF_IO_FALSE=
