  abi_sec_title="Output"

    abi_sec_spaces="                                                                      "
  abi_sec_tmp="${abi_sec_title}"
  while test "${abi_sec_tmp}" != ""; do
    abi_sec_spaces=`echo "${abi_sec_spaces}" | sed -e 's/^.//'`
    abi_sec_tmp=`echo "${abi_sec_tmp}" | sed -e 's/^.//'`
  done

  echo ""
  echo " =============================================================================="
  echo " === ${abi_sec_title}${abi_sec_spaces} ==="
  echo " =============================================================================="
  echo ""


# Output files
# Note: later, add tests/atconfig and tests/atlocal for test suite

    ac_config_files="$ac_config_files config.dump config.mk config.pc config.sh config/wrappers/wrap-fc src/incs/Makefile src/mods/Makefile src/16_hideleave/m_build_info.F90 tests/tests.env tests/tests-install.env Makefile src/Makefile src/01_gsl_ext/Makefile src/01_interfaces_ext/Makefile src/01_macroavnew_ext/Makefile src/01_qespresso_ext/Makefile src/02_clib/Makefile src/10_defs/Makefile src/12_hide_mpi/Makefile src/14_hidewrite/Makefile src/15_gpu_toolbox/Makefile src/16_hideleave/Makefile src/18_timing/Makefile src/27_toolbox_oop/Makefile src/28_numeric_noabirule/Makefile src/32_contract/Makefile src/32_util/Makefile src/42_geometry/Makefile src/42_geomoptim/Makefile src/42_nlstrain/Makefile src/42_parser/Makefile src/43_ptgroups/Makefile src/45_psp_parser/Makefile src/47_xml/Makefile src/49_gw_toolbox_oop/Makefile src/50_abitypes_defs/Makefile src/51_manage_cuda/Makefile src/51_manage_mpi/Makefile src/52_fft_mpi_noabirule/Makefile src/53_abiutil/Makefile src/53_ffts/Makefile src/53_spacepar/Makefile src/56_recipspace/Makefile src/56_xc/Makefile src/57_iovars/Makefile src/59_io_mpi/Makefile src/61_ionetcdf/Makefile src/62_cg_noabirule/Makefile src/62_iowfdenpot/Makefile src/62_occeig/Makefile src/62_poisson/Makefile src/62_wvl_wfs/Makefile src/63_bader/Makefile src/64_atompaw/Makefile src/65_nonlocal/Makefile src/65_psp/Makefile src/66_paw/Makefile src/66_wfs/Makefile src/67_common/Makefile src/68_dmft/Makefile src/68_gw/Makefile src/68_recursion/Makefile src/68_rsprc/Makefile src/69_bse/Makefile src/72_response/Makefile src/77_ddb/Makefile src/77_lwf/Makefile src/77_suscep/Makefile src/79_seqpar_mpi/Makefile src/83_cut3d/Makefile src/93_rdm/Makefile src/95_drive/Makefile src/98_main/Makefile src/libs/Makefile tests/Nightly/Makefile prereqs/Makefile prereqs/linalg/Makefile plugins/Makefile plugins/bigdft/Makefile plugins/etsf_io/Makefile plugins/fox/Makefile plugins/libxc/Makefile plugins/netcdf/Makefile plugins/wannier90/Makefile bindings/Makefile bindings/parser/Makefile doc/Makefile tests/Makefile"


