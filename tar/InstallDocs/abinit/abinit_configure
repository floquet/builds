    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: disabling ScaLAPACK support (MPI is disabled)" >&5
$as_echo "$as_me: WARNING: disabling ScaLAPACK support (MPI is disabled)" >&2;}
