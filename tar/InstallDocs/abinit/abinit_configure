    abi_plug_downloader="${WGET}"
    abi_plug_dlopts="--timeout=15 --tries=1 -q -O"
  elif test "${CURL}" != ""; then
    abi_plug_downloader="${CURL}"
    abi_plug_dlopts="--connect-time 15 --retry 1 -s -S -o"
  fi

    libxc_pkg_name="libxc-svn-r6071-fixed"

  libxc_pkg_string="LibXC library SVN revision 6071 (fixed for Abinit by YP)"


  if test -z "${CFGFLAGS_LIBXC}"; then
    CFGFLAGS_LIBXC=""
  fi

