else
  abi_fft_user_serial="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_fft_user_serial}" >&5
$as_echo "${abi_fft_user_serial}" >&6; }

      { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a MPI optimized FFT support" >&5
$as_echo_n "checking for a MPI optimized FFT support... " >&6; }
  if test "${enable_mpi}" = "yes"; then
    cat > conftest.$ac_ext <<_ACEOF
      program main

        choke me

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  abi_fft_user_mpi="yes"
else
  abi_fft_user_mpi="no"
