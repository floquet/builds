                  CFLAGS_OPTIM="-O2"
                  ;;
                standard)
                  enable_optim_opt="standard"
                  CFLAGS_OPTIM="-O2"
                  ;;
              esac   # [case: enable_optim, indent: 6, item: True]
              ;;
          esac   # [case: abi_cpu_spec, indent: 4, item: True]
          ;;
        10.0)
