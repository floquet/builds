$as_echo "${build_bigdft}" >&6; }

    if test "${enable_bigdft}" = "yes"; then

$as_echo "#define HAVE_BIGDFT 1" >>confdefs.h

  fi





     if test "${enable_bigdft}" = "yes"; then
  DO_TEST_BIGDFT_TRUE=
