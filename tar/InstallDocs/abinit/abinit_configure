                ;;
              no)
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: tarball MD5 check failed" >&5
$as_echo "$as_me: WARNING: tarball MD5 check failed" >&2;}
                ;;
            esac
          fi
        fi

                if test "${abi_plug_tarball}" = "yes"; then
          lib_bigdft_incs="-I\$(abinit_builddir)/plugins/bigdft"
          lib_bigdft_libs="-L\$(abinit_builddir)/plugins/bigdft -lbigdft -lpoissonsolver"
          build_bigdft="yes"
        else
          lib_bigdft_incs=""
          lib_bigdft_libs=""
          enable_bigdft="no"
          build_bigdft="no"
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not download BIGDFT plug-in tarball" >&5
$as_echo "$as_me: WARNING: could not download BIGDFT plug-in tarball" >&2;}
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: support for BIGDFT plug-in has been disabled" >&5
$as_echo "$as_me: WARNING: support for BIGDFT plug-in has been disabled" >&2;}
        fi
      fi
    else
      lib_bigdft_incs="${with_bigdft_includes}"
      lib_bigdft_libs="${with_bigdft_libs}"
      build_bigdft="no"
    fi

        if test "${lib_bigdft_incs}" != ""; then
      if test "${abi_fc_vendor}" = "sun"; then
        tmp_bigdft_incs=`echo "${lib_bigdft_incs}" | sed -e 's/-I/-M/g'`
        lib_bigdft_incs="${lib_bigdft_incs} ${tmp_bigdft_incs}"
      fi
    fi
  else
    enable_bigdft="no"
    build_bigdft="no"
  fi


    if test "${build_bigdft}" = "yes"; then


  if test "${abi_fc_vendor}" != "unknown"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: applying BigDFT tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&5
$as_echo "$as_me: applying BigDFT tricks (vendor: ${abi_fc_vendor}, version: ${abi_fc_version})" >&6;}
  fi

  CFGFLAGS_BIGDFT="${CFGFLAGS_BIGDFT} --disable-binaries --disable-libabinit --enable-libpoissonsolver"

    if test "${bigdft_tricks_bypass}" = "yes"; then
      build_bigdft="no"
    fi
  fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable the BIGDFT plug-in" >&5
$as_echo_n "checking whether to enable the BIGDFT plug-in... " >&6; }
