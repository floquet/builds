/* end confdefs.h.  */
#include <stdlib.h>
#include "mpi.h"
#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{

      int rc;

      MPI_Init(NULL,NULL);
      rc = MPI_Finalize();

  ;
  return 0;
}
