if test -z "${DO_BUILD_NETCDF_TRUE}" && test -z "${DO_BUILD_NETCDF_FALSE}"; then
  as_fn_error "conditional \"DO_BUILD_NETCDF\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
