    abi_env_MDFLAGS="${MDFLAGS}"

    abi_env_MPI_RUNNER="${MPI_RUNNER}"

    abi_env_NM="${NM}"

    abi_env_NVCC="${NVCC}"

    abi_env_NVCC_CFLAGS="${NVCC_CFLAGS}"

    abi_env_NVCC_CPPFLAGS="${NVCC_CPPFLAGS}"

    abi_env_NVCC_LDFLAGS="${NVCC_LDFLAGS}"

    abi_env_NVCC_LIBS="${NVCC_LIBS}"

    abi_env_PDFFLAGS="${PDFFLAGS}"

    abi_env_RANLIB="${RANLIB}"

    abi_env_SERIAL_RUNNER="${SERIAL_RUNNER}"

    abi_env_XPP="${XPP}"

    abi_env_XPPFLAGS="${XPPFLAGS}"

    abi_env_XPPFLAGS_DEBUG="${XPPFLAGS_DEBUG}"

    abi_env_XPPFLAGS_EXTRA="${XPPFLAGS_EXTRA}"

    abi_env_XPPFLAGS_OPTIM="${XPPFLAGS_OPTIM}"


  abi_saved_LIBS="${LIBS}"
  CPPFLAGS="${with_trio_includes} ${CPPFLAGS}"
  FCFLAGS="${with_trio_includes} ${FCFLAGS}"
  LDFLAGS="${FC_LDFLAGS}"
  LIBS="${with_trio_libs} ${LIBS}"
  ac_ext=${ac_fc_srcext-f}
ac_compile='$FC -c $FCFLAGS $ac_fcflags_srcext conftest.$ac_ext >&5'
ac_link='$FC -o conftest$ac_exeext $FCFLAGS $LDFLAGS $ac_fcflags_srcext conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_fc_compiler_gnu


    if test "${enable_trio}" = "yes"; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the requested transferable I/O support" >&5
$as_echo_n "checking for the requested transferable I/O support... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${with_trio_flavor}" >&5
$as_echo "${with_trio_flavor}" >&6; }

        abi_trio_iter=`echo "${with_trio_flavor}" | sed -e 's/+/\n/g' | sort -u | awk '{printf " %s",$1}'`
    abi_trio_tmp="${abi_trio_iter}"
    for abi_trio_flavor in ${abi_trio_iter}; do
      if test "${abi_trio_flavor}" = "etsf"; then
        abi_trio_tmp=`echo "${abi_trio_iter}" | sed -e 's/netcdf//'`
        abi_trio_tmp="netcdf ${abi_trio_tmp}"
      fi
    done
    abi_trio_iter="${abi_trio_tmp}"

    for abi_trio_flavor in ${abi_trio_iter}; do

      case "${abi_trio_flavor}" in

        etsf)
          if test "${abi_trio_netcdf_serial}" = "yes"; then

    abi_trio_etsf_has_incs="no"
  abi_trio_etsf_has_libs="no"
  abi_trio_etsf_serial="no"
  abi_trio_etsf_mpi="no"
  abi_trio_etsf_fcflags=""
  abi_trio_etsf_ldflags=""
  abi_trio_etsf_incs="${with_trio_includes}"
  abi_trio_etsf_libs="${with_trio_libs}"



    if test "${abi_trio_etsf_libs}" = ""; then
                    LIBS="-letsf_io_utils -letsf_io ${LIBS}"
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether we have a working ETSF_IO library" >&5
$as_echo_n "checking whether we have a working ETSF_IO library... " >&6; }
    cat > conftest.$ac_ext <<_ACEOF
      program main

        use etsf_io_low_level
        use etsf_io
        use etsf_io_tools
        character(len=etsf_charlen),allocatable :: atoms(:)
        integer :: ncid
        logical :: lstat
        type(etsf_io_low_error) :: err
        call etsf_io_tools_get_atom_names(ncid,atoms,lstat,err)

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  abi_trio_etsf_has_libs="yes"
else
  abi_trio_etsf_has_libs="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_trio_etsf_has_libs}" >&5
$as_echo "${abi_trio_etsf_has_libs}" >&6; }
    if test "${abi_trio_etsf_has_libs}" = "yes"; then
      abi_trio_etsf_has_incs="yes"
      abi_trio_etsf_libs="-letsf_io_utils -letsf_io"
    fi
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the specified ETSF_IO library works" >&5
$as_echo_n "checking whether the specified ETSF_IO library works... " >&6; }
    cat > conftest.$ac_ext <<_ACEOF
      program main

        use etsf_io_low_level
        use etsf_io
        use etsf_io_tools
        character(len=etsf_charlen),allocatable :: atoms(:)
        integer :: ncid
        logical :: lstat
        type(etsf_io_low_error) :: err
        call etsf_io_tools_get_atom_names(ncid,atoms,lstat,err)

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  abi_trio_etsf_has_incs="yes"; abi_trio_etsf_has_libs="yes"
else
  abi_trio_etsf_has_libs="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_trio_etsf_has_libs}" >&5
$as_echo "${abi_trio_etsf_has_libs}" >&6; }
  fi

    if test "${abi_trio_etsf_has_incs}" = "yes" -a \
          "${abi_trio_etsf_has_libs}" = "yes"; then
    abi_trio_etsf_serial="yes"
  fi

    if test "${enable_mpi_trio}" = "yes"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether ETSF_IO supports MPI I/O" >&5
$as_echo_n "checking whether ETSF_IO supports MPI I/O... " >&6; }
    cat > conftest.$ac_ext <<_ACEOF
      program main

        use etsf_io_low_level
        call etsf_io_low_open_create_par

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  abi_trio_etsf_mpi="yes"
else
  abi_trio_etsf_mpi="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_trio_etsf_has_libs}" >&5
$as_echo "${abi_trio_etsf_has_libs}" >&6; }
  fi

          else
            abi_trio_etsf_serial="no"
            abi_trio_etsf_mpi="no"
          fi
          if test "${abi_trio_etsf_serial}" = "yes"; then

$as_echo "#define HAVE_TRIO_ETSF 1" >>confdefs.h

                        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: disabling ETSF_IO plugin" >&5
$as_echo "$as_me: WARNING: disabling ETSF_IO plugin" >&2;}
            enable_etsf_io="no"

$as_echo "#define HAVE_ETSF_IO 1" >>confdefs.h

          else
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: falling back to ETSF_IO plugin" >&5
$as_echo "$as_me: WARNING: falling back to ETSF_IO plugin" >&2;}
            enable_etsf_io="yes"
          fi
          ;;

        fox)

    abi_trio_fox_has_incs="no"
  abi_trio_fox_has_libs="no"
  abi_trio_fox_serial="no"
  abi_trio_fox_mpi="no"
  abi_trio_fox_fcflags=""
  abi_trio_fox_ldflags=""
  abi_trio_fox_incs="${with_trio_includes}"
  abi_trio_fox_libs="${with_trio_libs}"

    if test "${abi_trio_fox_libs}" = ""; then
    LIBS="-lFoX_sax -lFoX_utils -lFoX_fsys -lFoX_common ${LIBS}"
