ac_compiler_gnu=$ac_cv_fc_compiler_gnu


    if test "${enable_linalg}" = "yes"; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the requested linear algebra support" >&5
$as_echo_n "checking for the requested linear algebra support... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${with_linalg_flavor}" >&5
$as_echo "${with_linalg_flavor}" >&6; }

    case "${with_linalg_flavor}" in

      asl)

    abi_linalg_asl_serial="no"
  abi_linalg_asl_mpi="no"
  abi_linalg_asl_fcflags=""
  abi_linalg_asl_ldflags=""
  abi_linalg_asl_incs=""
  abi_linalg_asl_libs=""
  abi_linalg_asl_deps=""

    if test "${with_linalg_libs}" = ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for zgemm in -lasl" >&5
$as_echo_n "checking for zgemm in -lasl... " >&6; }
if test "${ac_cv_lib_asl_zgemm+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lasl  $LIBS"
cat > conftest.$ac_ext <<_ACEOF
      program main
      call zgemm
      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  ac_cv_lib_asl_zgemm=yes
else
  ac_cv_lib_asl_zgemm=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_asl_zgemm" >&5
$as_echo "$ac_cv_lib_asl_zgemm" >&6; }
if test "x$ac_cv_lib_asl_zgemm" = x""yes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_LIBASL 1
_ACEOF

  LIBS="-lasl $LIBS"

fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for zhpev in -lasl" >&5
$as_echo_n "checking for zhpev in -lasl... " >&6; }
if test "${ac_cv_lib_asl_zhpev+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lasl  $LIBS"
cat > conftest.$ac_ext <<_ACEOF
      program main
      call zhpev
      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  ac_cv_lib_asl_zhpev=yes
else
  ac_cv_lib_asl_zhpev=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_asl_zhpev" >&5
$as_echo "$ac_cv_lib_asl_zhpev" >&6; }
if test "x$ac_cv_lib_asl_zhpev" = x""yes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_LIBASL 1
_ACEOF

  LIBS="-lasl $LIBS"

fi

    if test "${ac_cv_lib_asl_zgemm}" = "yes" -a \
            "${ac_cv_lib_asl_zhpev}" = "yes"; then
      abi_linalg_asl_serial="yes"
      abi_linalg_asl_libs="-lasl"
    fi
  else

    abi_linalg_user_serial="no"
  abi_linalg_user_mpi="no"

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a serial optimized linear algebra support" >&5
$as_echo_n "checking for a serial optimized linear algebra support... " >&6; }
  cat > conftest.$ac_ext <<_ACEOF
      program main

      call zgemm
      call zhpev

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  abi_linalg_user_serial="yes"
else
  abi_linalg_user_serial="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_linalg_user_serial}" >&5
$as_echo "${abi_linalg_user_serial}" >&6; }

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a MPI optimized linear algebra support" >&5
$as_echo_n "checking for a MPI optimized linear algebra support... " >&6; }
  if test "${enable_mpi}" = "yes" -a "${enable_scalapack}" = "yes"; then
    cat > conftest.$ac_ext <<_ACEOF
      program main

        call blacs_gridinit
        call pzheevx

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  abi_linalg_user_mpi="yes"
else
  abi_linalg_user_mpi="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  fi
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_linalg_user_mpi}" >&5
$as_echo "${abi_linalg_user_mpi}" >&6; }

    abi_linalg_asl_serial="${abi_linalg_user_serial}"
    abi_linalg_asl_mpi="${abi_linalg_user_mpi}"
    if test "${abi_linalg_user_serial}" = "yes"; then
      abi_linalg_asl_incs="${with_linalg_includes}"
      abi_linalg_asl_libs="${with_linalg_libs}"
    fi
  fi

        abi_linalg_serial="${abi_linalg_asl_serial}"
        abi_linalg_mpi="${abi_linalg_asl_mpi}"
        if test "${abi_linalg_serial}" = "yes"; then

$as_echo "#define HAVE_LINALG_ASL 1" >>confdefs.h

          lib_linalg_fcflags="${abi_linalg_asl_fcflags}"
          lib_linalg_ldflags="${abi_linalg_asl_ldflags}"
          lib_linalg_incs="${abi_linalg_asl_incs}"
          lib_linalg_libs="${abi_linalg_asl_libs}"
        fi
        ;;

      atlas)

    abi_linalg_atlas_serial="no"
  abi_linalg_atlas_mpi="no"
  abi_linalg_atlas_fcflags=""
  abi_linalg_atlas_ldflags=""
  abi_linalg_atlas_incs=""
  abi_linalg_atlas_libs=""
  abi_linalg_atlas_deps=""

    if test "${with_linalg_libs}" = ""; then
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for ATL_buildinfo in -latlas" >&5
$as_echo_n "checking for ATL_buildinfo in -latlas... " >&6; }
if test "${ac_cv_lib_atlas_ATL_buildinfo+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-latlas  $LIBS"
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
#ifdef __cplusplus
extern "C"
#endif
char ATL_buildinfo ();
#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{
return ATL_buildinfo ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  ac_cv_lib_atlas_ATL_buildinfo=yes
else
  ac_cv_lib_atlas_ATL_buildinfo=no
fi
rm -f core conftest.err conftest.$ac_objext \
