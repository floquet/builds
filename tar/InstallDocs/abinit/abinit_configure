    "src/32_contract/Makefile") CONFIG_FILES="$CONFIG_FILES src/32_contract/Makefile" ;;
    "src/32_util/Makefile") CONFIG_FILES="$CONFIG_FILES src/32_util/Makefile" ;;
    "src/42_geometry/Makefile") CONFIG_FILES="$CONFIG_FILES src/42_geometry/Makefile" ;;
    "src/42_geomoptim/Makefile") CONFIG_FILES="$CONFIG_FILES src/42_geomoptim/Makefile" ;;
    "src/42_nlstrain/Makefile") CONFIG_FILES="$CONFIG_FILES src/42_nlstrain/Makefile" ;;
    "src/42_parser/Makefile") CONFIG_FILES="$CONFIG_FILES src/42_parser/Makefile" ;;
    "src/43_ptgroups/Makefile") CONFIG_FILES="$CONFIG_FILES src/43_ptgroups/Makefile" ;;
    "src/45_psp_parser/Makefile") CONFIG_FILES="$CONFIG_FILES src/45_psp_parser/Makefile" ;;
