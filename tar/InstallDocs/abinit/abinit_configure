if test -z "${DO_TEST_VDWXC_TRUE}" && test -z "${DO_TEST_VDWXC_FALSE}"; then
  as_fn_error "conditional \"DO_TEST_VDWXC\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
