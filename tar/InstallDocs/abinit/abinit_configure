    conftest$ac_exeext conftest.$ac_ext
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_fft_user_threads}" >&5
$as_echo "${abi_fft_user_threads}" >&6; }

    abi_fft_fftw3_serial="${abi_fft_user_serial}"
    abi_fft_fftw3_mpi="${abi_fft_user_mpi}"
    if test "${abi_fft_fftw3_serial}" = "yes"; then
      abi_fft_fftw3_incs="${with_fft_includes}"
      abi_fft_fftw3_libs="${with_fft_libs}"
    fi
  fi

    if test "${abi_fft_fftw3_serial}" = "yes"; then
    if test "${with_fft_libs}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking for dfftw_init_threads in -lfftw3_threads" >&5
$as_echo_n "checking for dfftw_init_threads in -lfftw3_threads... " >&6; }
if test "${ac_cv_lib_fftw3_threads_dfftw_init_threads+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lfftw3_threads  $LIBS"
cat > conftest.$ac_ext <<_ACEOF
      program main
      call dfftw_init_threads
      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  ac_cv_lib_fftw3_threads_dfftw_init_threads=yes
else
  ac_cv_lib_fftw3_threads_dfftw_init_threads=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_fftw3_threads_dfftw_init_threads" >&5
$as_echo "$ac_cv_lib_fftw3_threads_dfftw_init_threads" >&6; }
if test "x$ac_cv_lib_fftw3_threads_dfftw_init_threads" = x""yes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_LIBFFTW3_THREADS 1
_ACEOF

  LIBS="-lfftw3_threads $LIBS"

fi

      if test "${ac_cv_lib_fftw3_threads_dfftw_init_threads}" = "yes"; then
        abi_fft_fftw3_threads="yes"
        abi_fft_fftw3_libs="-lfftw3_threads ${abi_fft_fftw3_libs}"
      fi
    else
      abi_fft_fftw3_threads="${abi_fft_user_threads}"
    fi
  fi

        abi_fft_serial="${abi_fft_fftw3_serial}"
        abi_fft_mpi="${abi_fft_fftw3_mpi}"
        abi_fft_threads="${abi_fft_fftw3_threads}"
        if test "${abi_fft_serial}" = "yes"; then

$as_echo "#define HAVE_FFT_FFTW3 1" >>confdefs.h

          if test "${abi_fft_threads}" = "yes"; then

$as_echo "#define HAVE_FFT_FFTW3_THREADS 1" >>confdefs.h

            lib_fft_flavor="fftw3-threads"
          else
            lib_fft_flavor="fftw3"
          fi
          if test "${with_fft_flavor}" = "fftw3-mkl"; then

$as_echo "#define HAVE_FFT_FFTW3_MKL 1" >>confdefs.h

          fi
          lib_fft_fcflags="${abi_fft_fftw3_fcflags}"
          lib_fft_ldflags="${abi_fft_fftw3_ldflags}"
          lib_fft_incs="${abi_fft_fftw3_incs}"
          lib_fft_libs="${abi_fft_fftw3_libs}"
        fi
        ;;

      mlib)

    abi_fft_mlib_serial="no"
  abi_fft_mlib_mpi="no"
  abi_fft_mlib_fcflags=""
  abi_fft_mlib_ldflags=""
  abi_fft_mlib_incs=""
  abi_fft_mlib_libs=""

