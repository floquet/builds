build_etsf_io
lib_etsf_io_libs
lib_etsf_io_incs
FCLIBS_ETSF_IO
FCFLAGS_ETSF_IO
CXXFLAGS_ETSF_IO
CFLAGS_ETSF_IO
CPPFLAGS_ETSF_IO
CFGFLAGS_ETSF_IO
etsf_io_pkg_string
etsf_io_pkg_name
DO_BUILD_NETCDF_FALSE
DO_BUILD_NETCDF_TRUE
DO_TEST_NETCDF_FALSE
DO_TEST_NETCDF_TRUE
build_netcdf
lib_netcdf_libs
lib_netcdf_incs
FCLIBS_NETCDF
FCFLAGS_NETCDF
CXXFLAGS_NETCDF
CFLAGS_NETCDF
