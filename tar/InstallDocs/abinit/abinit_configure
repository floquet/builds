      fcflags_opt_15_gpu_toolbox="${fcflags_opt_default}"
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: optimization for 15_gpu_toolbox is ${fcflags_opt_15_gpu_toolbox}" >&5
