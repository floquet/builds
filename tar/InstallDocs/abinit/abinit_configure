# Propagate "--enable-all-plugins"
if test "${enable_all_plugins}" = "yes"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: enabling all plug-ins" >&5
$as_echo "$as_me: enabling all plug-ins" >&6;}
  enable_bigdft="yes"
  enable_etsf_io="yes"
  enable_libxc="yes"
  enable_fox="yes"
  enable_netcdf="yes"
  enable_wannier90="yes"
fi
if test "${enable_all_plugins}" = "no"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: disabling all plug-ins" >&5
$as_echo "$as_me: disabling all plug-ins" >&6;}
  enable_bigdft="no"
  enable_etsf_io="no"
  enable_libxc="no"
  enable_fox="no"
  enable_netcdf="no"
  enable_wannier90="no"
fi

# Translate optimization level
abi_optflags_mode="${enable_optim}"

